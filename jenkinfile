pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
                git branch: 'main', url: 'https://github.com/chahardivya/jenkinpipeline.git'
            }
        }
        stage('build') {
            steps {
                echo 'Hello World'
                git branch: 'main', url: 'https://github.com/chahardivya/jenkinpipeline.git'
            }
        }
        stage('deploying') {
            steps {
                echo 'Hello World'
                git branch: 'main', url: 'https://github.com/chahardivya/jenkinpipeline.git'
            }
        }
    }
}
